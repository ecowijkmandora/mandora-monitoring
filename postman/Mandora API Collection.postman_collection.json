						{
							"listen": "test",
							"script": {
								"id": "0c23b580-b50a-48a8-826c-a5b8e5aee55f",
								"exec": [
									"const response = pm.response.json()",
									"",
									"pm.environment.set('JWT_TOKEN', response.jwt)"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
